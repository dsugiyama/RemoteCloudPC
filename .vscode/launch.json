{
	"version": "0.2.0",
	"configurations": [
		{
			"name": "Launch http",
			"type": "node",
			"request": "launch",
			"program": "${workspaceRoot}/js/http.js",
			"stopOnEntry": false,
			"args": [],
			"cwd": "${workspaceRoot}",
			"runtimeExecutable": null,
			"runtimeArgs": [
				"--nolazy"
